@model IEnumerable<Employee>
@{
    ViewData["Title"] = "Home Page";
}

    <h1> Employees</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Name</th>
                <th scope="col">Surname</th>
                <th scope="col">Gender</th>
                <th scope="col">Salary</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var empl in Model)
            {
                <tr>
                    <th scope="row">@empl.Id</th>
                    <td>@empl.Name</td>
                    <td>@empl.Surname</td>
                    <td>@empl.Gender</td>
                    <td>@empl.Salary</td>
                </tr>
            }
        </tbody>
    </table>